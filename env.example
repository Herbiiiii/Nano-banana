# Replicate API ключ
REPLICATE_API_TOKEN=your_replicate_api_token_here

# PostgreSQL настройки (для локального запуска без Docker)
POSTGRES_HOST=localhost
POSTGRES_PORT=5433  # Внешний порт изменен на 5433 из-за конфликта
POSTGRES_DB=nano_banana
POSTGRES_USER=nano_banana_user
POSTGRES_PASSWORD=nano_banana_pass

# MinIO настройки (для локального запуска без Docker)
MINIO_ENDPOINT=localhost:9002
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_BUCKET=nano-banana-images
MINIO_USE_SSL=false
MINIO_PUBLIC_URL=http://localhost:9002
# MinIO Console: http://localhost:9003

